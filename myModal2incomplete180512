    modal2.style.display = "none";
  }
}
</script>

<script>
// Handle ESC key (key code 27)
document.addEventListener('keyup', function(e) {
    if (e.keyCode == 27) {
        closeModal();
    }
    if (e.keyCode == 37) {    
        plusDivs2(-1)
    }
    if (e.keyCode == 39) {    
        plusDivs2(1)
    }
});
</script>

<script>
function openModal() {
  document.getElementById('myModal2').style.display = "block";
}

function closeModal() {
  document.getElementById('myModal2').style.display = "none";
}

var slideIndex = 1;
showDivs2(slideIndex);

function plusDivs2(n) {
  showDivs2(slideIndex += n);
}

function currentDiv2(n) {
  showDivs2(slideIndex = n);
}

function showDivs2(n) {
  var i;
  var x = document.getElementsByClassName("mySlides2");
  var placesTitle = document.getElementsByClassName("modal-places-title2");
  var placesDescription = document.getElementsByClassName("places-description2");
  var placesLocation = document.getElementsByClassName("location-marker2");
  var placesAddress = document.getElementsByClassName("places-address2");
  if (n > x.length) {slideIndex = 1}
  if (n < 1) {slideIndex = x.length}
  for (i = 0; i < x.length; i++) {
     x[i].style.display = "none";
  } 
  if (n > placesTitle.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesTitle.length}
  for (i = 0; i < placesTitle.length; i++) {
     placesTitle[i].style.display = "none";
  }
  if (n > placesDescription.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesDescription.length}
  for (i = 0; i < placesDescription.length; i++) {
     placesDescription[i].style.display = "none";
  }
  if (n > placesLocation.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesLocation.length}
  for (i = 0; i < placesLocation.length; i++) {
     placesLocation[i].style.display = "none";
  }
  if (n > placesAddress.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesAdress.length}
  for (i = 0; i < placesAddress.length; i++) {
     placesAddress[i].style.display = "none";
  }
  x[slideIndex-1].style.display = "block";
  placesTitle[slideIndex-1].style.display = "block";
  placesDescription[slideIndex-1].style.display = "block";
  placesLocation[slideIndex-1].style.display = "block";
  placesAddress[slideIndex-1].style.display = "block";
}
</script>

<script>
var mySwiper3 = new Swiper('.experiences-slider', {
  // Optional parameters
  pagination: '.pag-suites',
  paginationClickable: true,
  nextButton: '.swiper-button-next',
  prevButton: '.swiper-button-prev',
  freeMode: true,
  freeModeMomentumRatio: .75,
  freeModeMomentumVelocityRatio: 2,
  speed: 500,
   // Default parameters
  slidesPerView: 4.1,
  spaceBetween: 16,
  // Responsive breakpoints
  breakpoints: {
    // when window width is <= 320px
    320: {
      slidesPerView: 2.2,
      spaceBetween: 12
    },
    // when window width is <= 480px
    480: {
      slidesPerView: 2.2,
      spaceBetween: 12
    },
    // when window width is <= 640px
    640: {
      slidesPerView: 3,
      spaceBetween: 12
    },
    // when window width is <= 768px
    768: {
      slidesPerView: 3.5,
      spaceBetween: 12
    },
    // when window width is <= 1440px
    1440: {
      slidesPerView: 4.1,
      spaceBetween: 12
    }
  }
  
});
  
</script> 



