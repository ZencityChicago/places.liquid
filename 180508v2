<script>
$(document).ready(function(){
function(){
        $('#myModal').modal({keyboard: true});
    });
});
</script>

<script>
function openModal() {
  document.getElementById('myModal').style.display = "block";
}

function closeModal() {
  document.getElementById('myModal').style.display = "none";
}

var slideIndex = 1;
showDivs(slideIndex);

function plusDivs(n) {
  showDivs(slideIndex += n);
}

function currentDiv(n) {
  showDivs(slideIndex = n);
}

function showDivs(n) {
  var i;
  var x = document.getElementsByClassName("mySlides");
  var placesTitle = document.getElementsByClassName("places-title");
  var placesDescription = document.getElementsByClassName("places-description");
  var placesLocation = document.getElementsByClassName("location-marker");
  var placesAddress = document.getElementsByClassName("places-address");
  if (n > x.length) {slideIndex = 1}
  if (n < 1) {slideIndex = x.length}
  for (i = 0; i < x.length; i++) {
     x[i].style.display = "none";
  } 
  if (n > placesTitle.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesTitle.length}
  for (i = 0; i < placesTitle.length; i++) {
     placesTitle[i].style.display = "none";
  }
  if (n > placesDescription.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesDescription.length}
  for (i = 0; i < placesDescription.length; i++) {
     placesDescription[i].style.display = "none";
  }
  if (n > placesLocation.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesLocation.length}
  for (i = 0; i < placesLocation.length; i++) {
     placesLocation[i].style.display = "none";
  }
  if (n > placesAddress.length) {slideIndex = 1}
  if (n < 1) {slideIndex = placesAdress.length}
  for (i = 0; i < placesAddress.length; i++) {
     placesAddress[i].style.display = "none";
  }
  x[slideIndex-1].style.display = "block";
  placesTitle[slideIndex-1].style.display = "block";
  placesDescription[slideIndex-1].style.display = "block";
  placesLocation[slideIndex-1].style.display = "block";
  placesAddress[slideIndex-1].style.display = "block";
}
</script>



<script>
var mySwiper3 = new Swiper('.experiences-slider', {
  // Optional parameters
  pagination: '.pag-suites',
  paginationClickable: true,
  nextButton: '.swiper-button-next',
  prevButton: '.swiper-button-prev',
  freeMode: true,
  freeModeMomentumRatio: .75,
  freeModeMomentumVelocityRatio: 2,
  speed: 500,
   // Default parameters
  slidesPerView: 4.1,
  spaceBetween: 16,
  // Responsive breakpoints
  breakpoints: {
    // when window width is <= 320px
    320: {
      slidesPerView: 2.2,
      spaceBetween: 12
    },
    // when window width is <= 480px
    480: {
      slidesPerView: 2.2,
      spaceBetween: 12
    },
    // when window width is <= 640px
    640: {
      slidesPerView: 3,
      spaceBetween: 12
    },
    // when window width is <= 768px
    768: {
      slidesPerView: 3.5,
      spaceBetween: 12
    },
    // when window width is <= 1440px
    1440: {
      slidesPerView: 4.1,
      spaceBetween: 12
    }
  }
  
});
  
</script> 



